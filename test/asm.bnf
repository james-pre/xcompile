# AT & T assembly

number			= "(0x)?\d+";
identifier		= "\w+";
register		= "%\w+";
immediate		= "\$(0x)?\d+";
whitespace		= "[ \t]+";
line_terminator	= "[\n;]+";
comment			= "#.*";

operand = register | immediate | number;

operand_list = operand, {",", operand};

instruction	= identifier, [operand_list];

instruction_list = (instruction | comment), {line_terminator, instruction | comment};
